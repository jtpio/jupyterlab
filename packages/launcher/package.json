  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^4.0.0-alpha.0",
    "@jupyterlab/translation": "^4.0.0-alpha.0",
    "@jupyterlab/ui-components": "^4.0.0-alpha.15",
    "@lumino/algorithm": "^1.9.1",
    "@lumino/commands": "^1.19.0",
    "@lumino/coreutils": "^1.11.1",
    "@lumino/disposable": "^1.10.1",
    "@lumino/properties": "^1.8.1",
    "@lumino/widgets": "^1.30.0",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "rimraf": "~3.0.0",
    "typedoc": "~0.21.2",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "styleModule": "style/index.js"
}
